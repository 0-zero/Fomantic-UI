/*-------------------
        Theme
--------------------*/

/* To override a theme for an individual element
   specify theme name below
*/

/* Global */

@theme   : 'default';

/* Elements */

@button : 'default';
// Example Uncomment Below
@button   : 'github';

@icon     : 'default';
@divider  : 'default';
@header   : 'default';
@icon     : 'default';
@image    : 'default';
@input    : 'default';
@label    : 'default';
@loader   : 'default';
@progress : 'default';
@reveal   : 'default';
@segment  : 'default';
@step     : 'default';

/* Collections */

@breadcrumb : 'default';
@form       : 'default';
@grid       : 'default';
@menu       : 'default';
@message    : 'default';
@table      : 'default';

/* Modules */

@accordion  : 'default';
@chatroom   : 'default';
@checkbox   : 'default';
@dropdown   : 'default';
@modal      : 'default';
@nag        : 'default';
@popup      : 'default';
@rating     : 'default';
@search     : 'default';
@shape      : 'default';
@sidebar    : 'default';
@tab        : 'default';
@transition : 'default';
@video      : 'default';

/* Views */

@comment   : 'default';
@feed      : 'default';
@item      : 'default';
@list      : 'default';
@statistic : 'default';


/* Import */

@elementTheme : @@element;
@import "theme.include";
